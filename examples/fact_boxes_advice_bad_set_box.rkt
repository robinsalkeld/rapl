(lambda (f a)
  ((file "around.ttpl") 
   ((file "call.ttpl") 'fact_boxes)
   (lambda (proceed ybox) 
     (let ([y (unbox ybox)])
       (seq (set-box! ybox (+ y -1))
            (let ([result (proceed ybox)])
              (seq (write "y" y)
                   (seq (write "result" result)
                        result))))))
   f a))
