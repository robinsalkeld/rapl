(lambda (thunk)
  ((file "around.ttpl") 
   ((file "call.ttpl") 'fact_helper)
   (lambda (proceed)
     (lambda (ybox) 
       (let ([y (unbox ybox)])
         (seq (set-box! ybox (+ y -1))
              (let ([result (proceed ybox)])
                (seq (write "y" y)
                     (seq (write "result" result)
                          result)))))))
   thunk))
