(let ([fact_recursive 
       ((file "Z.ttpl")
        (lambda (fact) 
          (tag 'fact_boxes
               (lambda (bx)
                 (let ([x (unbox bx)])
                   (if (equal? x 0) 
                       (seq (set-box! bx 42)
                            1)
                       (seq (set-box! bx (+ x -1))
                            (* x (fact bx)))))))))])
  (lambda (x) (fact_recursive (box x))))
